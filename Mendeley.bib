keywords = {Energy Analysis Program (EAP),cross-national comparisons,energy,environmental load,industrial ecology,materials flow analysis (MFA)},
number = {1},
pages = {259--275},
title = {{Pursuing More Sustainable Consumption by Analyzing Household Metabolism in European Countries and Cities}},
url = {http://onlinelibrary.wiley.com/doi/10.1162/1088198054084662/abstract{\%}5Cnhttp://doi.wiley.com/10.1162/1088198054084662},
