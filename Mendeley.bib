isbn = {0921-3449},
issn = {09213449},
journal = {Resources, Conservation and Recycling},
keywords = {Life-extension,Light-weighting,Material efficiency,Re-use,Remanufacturing},
language = {en},
month = {jan},
number = {3},
pages = {362--381},
publisher = {Elsevier B.V.},
shorttitle = {Material efficiency},
title = {{Material efficiency: A white paper}},
