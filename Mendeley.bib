journal = {Waste and Resource Management},
keywords = {corporate responsibility,government,waste management},
number = {1},
pages = {3--6},
title = {{Resource efficiency in construction—a perspective from the waste resources sector}},
url = {http://www.atypon-link.com/TELF/doi/abs/10.1680/warm.2008.161.1.3},
volume = {161},
year = {2008}
}
