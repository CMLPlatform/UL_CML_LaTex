isbn = {0048-7333},
issn = {00487333},
journal = {Research Policy},
keywords = {Evolutionary theory,Multi-level analysis,Regime shifts,Sailing ships and steamships,Technological transitions},
number = {8-9},
pages = {1257--1274},
pmid = {905},
title = {{Technological transitions as evolutionary reconfiguration processes: a multi-level perspective and a case-study}},
url = {http://www.sciencedirect.com/science/article/pii/S0048733302000628},
volume = {31},
year = {2002}
