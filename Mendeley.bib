keywords = {International dimensions,Japan,Photovoltaics,Technological Innovation Systems,The Netherlands},
pages = {200--210},
pmid = {421},
publisher = {Elsevier Ltd},
title = {{A comparative analysis of Photovoltaic Technological Innovation Systems including international dimensions: The cases of Japan and the Netherlands}},
