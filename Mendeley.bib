journal = {Bioresource Technology},
keywords = {Activated carbon,Chemical activation,Optimization,Response surface methodology,Scanning electron microscope},
mendeley-tags = {Activated carbon,Chemical activation,Optimization,Response surface methodology,Scanning electron microscope},
